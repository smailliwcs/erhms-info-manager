<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Wizards="clr-namespace:ERHMS.Desktop.ViewModels.Wizards"
                    xmlns:System="clr-namespace:System;assembly=mscorlib">
    <System:String x:Key="{x:Type Wizards:CreateViewWizard+ChooseTypeStep}">
        What type of form do you want to create?
    </System:String>
    <System:String x:Key="{x:Type Wizards:CreateViewWizard+CreateBlankStep}">
        Create a blank form
    </System:String>
    <System:String x:Key="{x:Type Wizards:CreateViewWizard+CreateTemplateBasedStep}">
        Create a form based on a template
    </System:String>
    <System:String x:Key="{x:Type Wizards:CreateViewWizard+CopyExistingStep}">
        Copy an existing form
    </System:String>
    <System:String x:Key="{x:Type Wizards:CreateViewWizard+OpenStep}">
        Form has been created
    </System:String>
</ResourceDictionary>
