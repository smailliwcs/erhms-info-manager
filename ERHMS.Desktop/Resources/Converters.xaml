<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Converters="clr-namespace:ERHMS.Desktop.Converters">
    <ResourceDictionary x:Key="WizardStepTitles"
                        Source="/Views/Wizards/StepTitles.xaml" />
    <Converters:BoolInverter x:Key="BoolInverter" />
    <Converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
    <Converters:BoolToVisibilityInverter x:Key="BoolToVisibilityInverter" />
    <Converters:IconToImageSourceConverter x:Key="IconToImageSourceConverter" />
    <Converters:IconToNameConverter x:Key="IconToNameConverter" />
    <Converters:ObjectToVisibilityConverter x:Key="ObjectToVisibilityConverter" />
    <Converters:PageCountToVisibilityConverter x:Key="PageCountToVisibilityConverter" />
    <Converters:ObjectToResourceConverter x:Key="WizardStepToTitleConverter"
                                          Resources="{StaticResource WizardStepTitles}" />
</ResourceDictionary>
